# Environment Variables for SMS Auto-Bot System
# Copy this file to .env and fill in your actual credentials

# SMS Website Admin Credentials (สำหรับ Bot Login)
VITE_SMS_ADMIN_USERNAME=your_username_here
VITE_SMS_ADMIN_PASSWORD=your_password_here

# Bot Configuration
VITE_SMS_BASE_URL=https://web.smsup-plus.com
VITE_SMS_BOT_HEADLESS=false

# Supabase Configuration
# Replace with your actual Supabase project URL and keys
VITE_SUPABASE_URL=your_supabase_url_here
SUPABASE_CLIENT_API_KEYY=your_supabase_publishable_key_here
VITE_SUPABASE_SERVICE_KEY=your_supabase_secret_key_here
VITE_SMS_BOT_TIMEOUT=30000

# Environment Mode (development = simulation, production = real Puppeteer)
VITE_BOT_MODE=development

# ANTS OTP Service Configuration
# จากเว็บ https://web.smsup-plus.com สำหรับ OTP Verification
VITE_ANTS_OTP_BASE_URL=https://web.smsup-plus.com
VITE_ANTS_OTP_LOGIN_USERNAME=your_ants_username
VITE_ANTS_OTP_LOGIN_PASSWORD=your_ants_password

# OTP Configuration
VITE_OTP_EXPIRES_IN_MINUTES=5
VITE_OTP_MAX_ATTEMPTS=3
VITE_OTP_RESEND_COOLDOWN_SECONDS=60
